<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="RefOntoUML.presentation.RefOntoUMLModelWizardID"
            name="%_UI_RefOntoUMLModelWizard_label"
            class="RefOntoUML.presentation.RefOntoUMLModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RefOntoUMLModelFile.gif">
         <description>%_UI_RefOntoUMLModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="RefOntoUML.presentation.RefOntoUMLEditorID"
            name="%_UI_RefOntoUMLEditor_label"
            icon="icons/full/obj16/RefOntoUMLModelFile.gif"
            extensions="refontouml"
            class="RefOntoUML.presentation.RefOntoUMLEditor"
            contributorClass="RefOntoUML.presentation.RefOntoUMLActionBarContributor">
      </editor>
   </extension>

</plugin>
